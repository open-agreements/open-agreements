{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "installCommand": "npm ci --include=dev",
  "buildCommand": "npm run build:site:vercel",
  "outputDirectory": "_site",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/a2a.ts": {
      "maxDuration": 30,
      "includeFiles": "{content/templates,content/external,dist}/**"
    },
    "api/mcp.ts": {
      "maxDuration": 30,
      "includeFiles": "{content/templates,content/external,dist}/**"
    },
    "api/download.ts": {
      "maxDuration": 30,
      "includeFiles": "{content/templates,content/external,dist}/**"
    }
  }
}
